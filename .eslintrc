{
     "extends": "airbnb/base",
     "rules": {
        "no-console":0,
        "eol-last": 0,
        "quotes": 0,
        "semi": 0,
        "indent": 0,
        "arrow-spacing": 0,
        "max-len": 0,
        "spaced-comment": 0,
        "comma-spacing": 0,
        "comma-dangle": 0,
        "func-names": 0,
        "no-param-reassign": 0
    },
    "env": {
        "browser": true,
        "node": false
    }
 }
